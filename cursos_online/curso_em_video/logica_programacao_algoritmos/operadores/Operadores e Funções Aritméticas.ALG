algoritmo "Aritmética"
//  
//  
var
   a,b,c,d,f,g,h:real
inicio
      a <- Abs(5*-3)
      b <- Exp(2,10)
      c <- Int(Pi)
      d <- Raizq(3)
      f <- sen(Pi/6)
      g <- cos(Pi/3)
      h <- tan(grauprad(45))
        Escreval(a)
        Escreval(b)
        Escreval(c)
        Escreval(d)
        Escreval(f)
        Escreval(g)
        Escreval(h)
fimalgoritmo